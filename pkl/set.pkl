// Field helper functions for go-mapper-gen - v0.6.0
@go.Package { name = "github.com/toniphan21/go-mapper-gen/pkg/pkl/set" }
module gomappergen.set

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.11.1#/go.pkl"

import "mapper.pkl"

function nil_if_zero(): mapper.FieldInterceptor = new mapper.FieldInterceptor {
  type = "nil-if-zero"
}

function use_function(symbol: String): mapper.FieldInterceptor = new mapper.FieldInterceptor {
  type = "use-function"
  options {
    ["symbol"] = symbol
  }
}

function via_function(symbol: String): mapper.FieldInterceptor = use_function(symbol)

function use_method(variableSymbol: String, methodName: String): mapper.FieldInterceptor = new mapper.FieldInterceptor {
  type = "use-function"
  options {
    ["symbol"] = variableSymbol
    ["method"] = methodName
  }
}

function via_method(variableSymbol: String, methodName: String): mapper.FieldInterceptor = use_method(variableSymbol, methodName)
