// Configuration for go-mapper-gen - v0.7.0
@go.Package { name = "github.com/toniphan21/go-mapper-gen/pkg/pkl" }
module gomappergen.Config

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.11.1#/go.pkl"
import "mapper.pkl"

typealias PackagePath = String

class Output {
  package: String?
  file_name: String?
  test_file_name: String?
}

class All {
  output: Output
}

open class BasePackage extends mapper.Package {
  output: Output?
}

class Package extends BasePackage {
  priorities: Mapping<Int, Package>
}

// ---

all: All = new All {
  output {
    package = "{CurrentPackageName}"
    file_name = "gen_mapper.go"
    test_file_name = "gen_mapper_test.go"
  }
}

converter: mapper.Converter = new mapper.Converter {}

packages: Mapping<PackagePath, Package>
